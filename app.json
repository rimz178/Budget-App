{
	"expo": {
		"name": "BudgetApp",
		"slug": "BudgetApp",
		"version": "1.0.0",
		"orientation": "portrait",
		"scheme": "budgetapp",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"backgroundColor": "#ffffff"
			},
			"edgeToEdgeEnabled": true
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [],
		"experiments": {
			"typedRoutes": true
		}
	}
}
